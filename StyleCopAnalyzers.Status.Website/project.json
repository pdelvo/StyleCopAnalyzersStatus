{
  "webroot": "wwwroot",
  "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNetCore.Diagnostics": "1.0.0-rc2-*",
    "Microsoft.AspNetCore.Mvc": "1.0.0-rc2-*",
    "Microsoft.AspNetCore.Mvc.TagHelpers": "1.0.0-rc2-*",
    "Microsoft.AspNetCore.Razor.Runtime": "1.0.0-rc2-*",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc2-*",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0-rc2-*",
    "Microsoft.Extensions.Logging.Console": "1.0.0-rc2-*",
    "Microsoft.Extensions.Logging.Debug": "1.0.0-rc2-*",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc2-*",
    "Microsoft.Extensions.Configuration.UserSecrets": "1.0.0-rc2-*",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.0-rc2-*",
    "Microsoft.Extensions.Caching.Abstractions": "1.0.0-rc2-*",
	  "Microsoft.Extensions.Caching.Memory": "1.0.0-rc2-*",
    "Newtonsoft.Json": "8.0.3",
    "StyleCopAnalyzers.Common": "1.0.0-*",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },
  "compilationOptions": {
    "xmlDoc": "true",
    "additionalArguments": [ "/ruleset:../StyleCop.Analyzers.ruleset", "/additionalfile:../stylecop.json" ]
  },
   "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "portable-dnxcore50+net45+win8+wp8+wpa81",
        "portable-net45+win8+wpa81",
        "dotnet5.6"
      ]
    }
   },
  "content": [
    "Views",
    "wwwroot",
    "web.config",
    "appsettings.json"
  ],
  "exclude": [
    "wwwroot",
    "node_modules"
  ],
  "publishExclude": [
    "**.user",
    "**.vspscc"
  ],
  "scripts": {
    "prepublish": [ "npm install", "bower install", "gulp clean", "gulp min" ]
  }
}
